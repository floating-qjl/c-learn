#define _CRT_SECURE_NO_WARNINGS 1
#include <stdio.h>
#include <string.h>
void swap1(int x, int y)
//x,y为形参
{
	int c = 0;
	c = x;
	x = y;
	y = c;
}
void swap2(int* pa, int* pb)
{
	int c = 0;
	c = *pa;
	*pa = *pb;
	*pb = c;
}
int main()
{
	int a = 30;
	int b = 10;	
	printf("a=%d b=%d\n", a, b);
	swap1(a, b);
	printf("a=%d b=%d\n", a, b);
	swap2(&a, &b);
	printf("a=%d b=%d\n", a, b);
	return 0;
}
