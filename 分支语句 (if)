#include<stdio.h>
#include<string.h>
int main()
{
	int san = 0;
	printf("您的san值为:");
	scanf("%d", &san);
	if (100 == san)
	//=为赋值    ==为等于
		printf("正常");
	else if (san >= 60 && san < 100)
		printf("紊乱");
	else
	{
		if (san < 60)
			if (san > 0)
				printf("疯狂");
			else
				printf("死亡");
				//else与最近的if搭配
	}
	return 0;
}
